 public static int mostFrequent(int[] arr, int n) {
      HashMap<Integer,Integer> map = new HashMap<>();
      for(int i = 0; i < n; i++)
        {
          map.put(arr[i],map.getOrDefault(arr[i],0)+1);
        }
      int max = 0;
      int val = 0;
      for(int x : map.keySet())
        {
          if(map.get(x)>max)
          {
            max=map.get(x);
            val = x;
          }
        }
      return val;
      }
