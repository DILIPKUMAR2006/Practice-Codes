class Solution 
{
    public int mostWordsFound(String[] sentences) 
    {
        int max = 0;
        for(int i = 0; i < sentences.length; i++)
        {
            String[] str = sentences[i].split("\\s");
            int current_length = str.length;
            if(current_length > max)
                max = current_length;
        }
        return max;
    }
}
