class Solution {
    public boolean judgeCircle(String moves) 
    {
        int x = 0, y = 0;
        for(int i = 0; i < moves.length(); i++)
        {
            char c = moves.charAt(i);
            switch(c)
            {
            case 'R':
            x++; break;
            case 'L':
            x--; break;
            case 'U':
            y++; break;
            case 'D':
            y--;
            }
        }
        return x==0 && y==0;
    }
}
