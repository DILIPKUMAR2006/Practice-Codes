class Solution 
{
    public int finalValueAfterOperations(String[] operations) 
    {
        int sum = 0;
        for(int i = 0; i < operations.length; i++)
        {
            if(operations[i].charAt(0) == '+' || operations[i].charAt(2) == '+')
                sum++;
            else 
                sum--;
        }
        return sum;
    }
}
