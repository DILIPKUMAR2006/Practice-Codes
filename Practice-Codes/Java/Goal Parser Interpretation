class Solution 
{
    public String interpret(String command) 
    {
        StringBuilder str = new StringBuilder();
        for(int i = 0; i < command.length(); i++)
        {
            char c = command.charAt(i);
            if(c == '(' && command.charAt(i+1) == ')')
                str.append("o");
            else if(c != '(' && c != ')')
                str.append(c);
        }
        return str.toString();
    }
}
